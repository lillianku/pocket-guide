<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <% include ./partials/head.ejs %>
    <title>Pocket Guide: Places</title>
  </head>
  <body>
    <h1>Places</h1>
    <% include ./partials/navbar.ejs %>

    <div class="board">
        <% templatePlaces.forEach((place) => {%>
          <div style="border: 1px solid black; margin: 5px">
              <h3><%=place.city%>, <%=place.country%></h3>
              <h4><%=place.url%></h4>
              <a href="/places/<%=place._id%>">View More</a>
          </div>
        <%})%>
      </div>

  </body>
</html>
